             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="DonusumIslemleri.MainPage">

    <StackLayout>
        <Button x:Name="translationPage" Text="Translation Örneği" Clicked="translationPage_Clicked"/>
        <Button x:Name="textEffectsPage" Text="Text Effect Örneği" Clicked="textEffectsPage_Clicked"/>
